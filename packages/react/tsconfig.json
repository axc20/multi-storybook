{
  "compilerOptions": {
    "outDir": "lib",
    "target": "ESNext",
    "moduleResolution": "node",
    "declaration": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "jsx": "react",
    "strict": true
  },
  "include": ["src", "../notes/src/Abdomination.examples.tsx"],
  "exclude": ["node_modules", "lib"]
}
