{
  "name": "multi-storybook",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">= 16.14.0",
    "yarn": "^1.22"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "presb:notes": "node tools/generateNotesMdx.mjs",
    "sb:notes": "start-storybook -c packages/notes/.storybook -p 4700 --quiet",
    "sb:svelte": "node node_modules/@storybook/svelte/bin -c packages/svelte/.storybook -p 4800 --quiet",
    "deploy": "storybook-to-ghpages --packages packages -o=./storybook-static",
    "postdeploy": "node tools/cleanup.mjs",
    "start": "http-server storybook-static"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@storybook/addon-a11y": "6.4.22",
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-interactions": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/addon-svelte-csf": "2.0.3",
    "@storybook/addons": "6.4.22",
    "@storybook/api": "6.4.22",
    "@storybook/builder-webpack5": "6.4.22",
    "@storybook/client-api": "6.4.22",
    "@storybook/client-logger": "6.4.22",
    "@storybook/components": "6.4.22",
    "@storybook/core-events": "6.4.22",
    "@storybook/manager-webpack5": "6.4.22",
    "@storybook/react": "6.4.22",
    "@storybook/storybook-deployer": "2.8.11",
    "@storybook/svelte": "6.4.22",
    "@storybook/theming": "6.4.22",
    "@storybook/web-components": "6.4.22",
    "@types/jest": "27.4.1",
    "@types/prettier": "2.6.0",
    "autoprefixer": "10.4.5",
    "babel-loader": "8.2.5",
    "chalk": "5.0.1",
    "globby": "13.1.1",
    "http-server": "14.1.0",
    "jest": "27.5.1",
    "lit-html": "2.2.3",
    "lodash-es": "4.17.21",
    "postcss": "8.4.13",
    "postcss-flexbugs-fixes": "5.0.2",
    "prettier": "2.6.2",
    "prism-react-renderer": "1.3.1",
    "ramda": "0.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-live": "3.1.0",
    "storybook-addon-themes": "6.1.0",
    "svelte": "3.48.0",
    "svelte-loader": "3.1.2",
    "ts-jest": "27.1.4",
    "typescript": "4.6.4",
    "webpack": "5.72.0"
  }
}
