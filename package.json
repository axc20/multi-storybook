{
  "name": "multi-storybook",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">= 16.14.0",
    "yarn": "^1.22"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "presb:notes": "node tools/generateNotesMdx.mjs",
    "sb:notes": "start-storybook -c packages/notes/.storybook -p 4700 --quiet",
    "sb:react": "node node_modules/@storybook/react/bin -c packages/react/.storybook -p 4750 --quiet",
    "sb:svelte": "node node_modules/@storybook/svelte/bin -c packages/svelte/.storybook -p 4800 --quiet",
    "deploy": "storybook-to-ghpages --packages packages -o=./storybook-static",
    "postdeploy": "node tools/cleanup.mjs",
    "start": "http-server storybook-static",
    "wasteland:eslint": "eslint the-wasteland",
    "wasteland:eslint:ci": "eslint the-wasteland --format=html --output-file=the-wasteland/eslintReport.html"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@storybook/addon-a11y": "6.5.5",
    "@storybook/addon-actions": "6.5.5",
    "@storybook/addon-essentials": "6.5.5",
    "@storybook/addon-interactions": "6.5.5",
    "@storybook/addon-links": "6.5.5",
    "@storybook/addon-svelte-csf": "2.0.3",
    "@storybook/addons": "6.5.5",
    "@storybook/api": "6.5.5",
    "@storybook/builder-webpack5": "6.5.5",
    "@storybook/client-api": "6.5.5",
    "@storybook/client-logger": "6.5.5",
    "@storybook/components": "6.5.5",
    "@storybook/core-events": "6.5.5",
    "@storybook/manager-webpack5": "6.5.5",
    "@storybook/react": "6.5.5",
    "@storybook/storybook-deployer": "2.8.11",
    "@storybook/svelte": "6.5.5",
    "@storybook/theming": "6.5.5",
    "@storybook/web-components": "6.5.5",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@types/jest": "27.4.1",
    "@types/prettier": "2.6.0",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "autoprefixer": "10.4.5",
    "babel-loader": "8.2.5",
    "chalk": "5.0.1",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-mdx": "1.17.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "globby": "13.1.1",
    "http-server": "14.1.0",
    "husky": "8.0.1",
    "jest": "27.5.1",
    "lint-staged": "12.4.2",
    "lit-html": "2.2.3",
    "lodash-es": "4.17.21",
    "postcss": "8.4.13",
    "postcss-flexbugs-fixes": "5.0.2",
    "prettier": "2.6.2",
    "prism-react-renderer": "1.3.1",
    "ramda": "0.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "18.1.0",
    "react-live": "3.1.0",
    "require-from-string": "2.0.2",
    "storybook-addon-themes": "6.1.0",
    "styled-components": "5.3.5",
    "svelte": "3.48.0",
    "svelte-loader": "3.1.2",
    "ts-jest": "27.1.4",
    "typescript": "4.6.4",
    "webpack": "5.72.0"
  },
  "lint-staged": {},
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
